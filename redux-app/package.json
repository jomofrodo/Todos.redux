{
  "name": "Todos-redux",
  "version": "0.0.0",
  "description": "Netazoic Todos/Redux based on TodoMVC",
  "scripts": {
    "start": "node server.js",
    "test": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "stats": "webpack --profile --json > stats.json",
    "build": "webpack",
    "dev": "webpack-dev-server --devtool eval --progress --colors --content-base build",
    "deploy": "cp -r build/* www/static"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jomofrodo/Todos.redux.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jomofrodo/Todos.redux/issues"
  },
  "homepage": "http://netazoic.com",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "classnames": "^2.1.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.2.1",
    "redux": "^3.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.3.13",
    "clean-webpack-plugin": "^0.1.8",
    "cross-env": "^1.0.7",
    "expect": "^1.8.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^5.6.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.7",
    "style-loader": "^0.12.3",
    "todomvc-app-css": "^2.0.1",
    "webpack": "^1.9.11",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.9.1",
    "node-uuid": "^1.4.7"
  }
}
